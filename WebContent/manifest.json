{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.ui.demo.walkthrough",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "http://localhost:8081/https://services.odata.org/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.demo.walkthrough.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.walkthrough.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "mainService"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "sap.ui.demo.walkthrough.view",
				"controlId": "app",
				"viewType": "XML",
				"transition": "slide",
				"async": true
			},
			"routes": [
				{
					"name": "categoriesMaster",
					"pattern":"",
					"target": "categoriesMaster"
				},
				{
					"name": "productListDetail",
					"pattern":"productList/:categoryID:",
					"target": ["categoriesMaster", "productListDetail"]
				},
				{
					
					"name": "productDetails",
					"pattern":"productList/:categoryID:/productDetails/:productID:",
					"target": ["categoriesMaster", "productDetails"]
				}
			],
			"targets":{
				"categoriesMaster": {
					"viewName": "CategoriesMaster",
					"controlAggregation": "masterPages",
					"viewLevel": 0
				},
				"productListDetail": {
					"viewName": "ProductListDetail",
					"controlAggregation": "detailPages",
					"viewLevel": 0
				},
				"productDetails": {
					"viewName": "ProductDetails",
					"controlAggregation": "detailPages",
					"viewLevel": 1
				}
			}
		}
	}
}
